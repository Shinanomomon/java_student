
public class TestArg {
	public static void main(String[] args) {
		int n = args.length;

		if(n==0) {
			System.err.println("Argument cannot be empty.");
			System.exit(0);
		}
		System.out.println("n = " + n);
		System.out.println(args[0]);
		System.out.println(args[1]);
		
		//System.out.println("n = " + n);
		//System.out.println("n = ".concat(n+""));
		
	}
}
